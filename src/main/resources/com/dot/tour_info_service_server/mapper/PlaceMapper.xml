<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dot.tour_info_service_server.mapper.PlaceMapper">
<!--    장소 등록-->
    <insert id="placeRegist">
        insert into place (name,lng,lat,road_address,local_address,eng_address,category)
        values(#{name},#{lng},#{lat},#{roadAddress},#{localAddress},#{engAddress},#{category});
    </insert>
</mapper>